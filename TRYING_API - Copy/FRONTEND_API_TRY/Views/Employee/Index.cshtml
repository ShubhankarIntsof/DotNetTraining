
@{
    ViewBag.Title = "Index";
}

<h2>FindALL</h2>

<div align="center">
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Salary</th>
        </tr>
    @foreach (var item in ViewBag.ListEmployees)
    {
    <tr>
        <td>@item.Empid</td>
        <td>@item.Empname</td>
        <td>@item.Email</td>
        <td>@item.Age</td>
        <td>@item.Salary</td>
    </tr>
        }
    </table>
</div>

